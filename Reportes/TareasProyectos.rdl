  <DataSets>
    <DataSet Name="Proyectos">
      <Query>
        <DataSourceName>Proyectos</DataSourceName>
        <CommandText>SELECT 
    Proyecto.Nombre AS NombreProyecto,
    Tarea.Estado,
    COUNT(Tarea.Id) AS CantidadTareas
FROM 
    Tarea
JOIN 
    Proyecto ON Tarea.ProyectoId = Proyecto.Id
GROUP BY 
    Proyecto.Nombre, Tarea.Estado
ORDER BY 
    Proyecto.Nombre, Tarea.Estado;</CommandText>
      </Query>
      <Fields>
        <Field Name="NombreProyecto">
          <DataField>NombreProyecto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Estado">
          <DataField>Estado</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CantidadTareas">
          <DataField>CantidadTareas</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
